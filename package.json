		"graphql-tools": "^0.10.0",
		"lodash": "^4.17.4",
		"sequelize": "^3.30.2",
		"sqlite": "^2.3.0",
		"subscriptions-transport-ws": "^0.7.0"
	},
	"devDependencies": {
		"ava": "^0.18.1",
		"babel-cli": "6.16.0",
		"babel-core": "^6.22.1",
		"babel-eslint": "^7.1.1",
		"babel-plugin-inline-import": "^2.0.4",
		"babel-preset-es2015": "^6.22.0",
		"babel-preset-stage-2": "^6.22.0",
