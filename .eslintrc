{
	"extends": [
		"eslint:recommended",
		"plugin:import/errors",
		"plugin:import/warnings"
	],
	"plugins": [
		"import",
		"babel"
	],
	"rules": {
		"import/no-unresolved": [2, { "commonjs": true, "amd": true }],
		"import/named": 2,
		"import/namespace": 2,
		"import/default": 2,
		"import/export": 2,
		"babel/new-cap": 1,
		"babel/no-await-in-loop": 1,
		"babel/flow-object-type": 1,
		"babel/no-invalid-this": 1
	},
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module"
	},
	"env": {
		"node": true
	}
}
